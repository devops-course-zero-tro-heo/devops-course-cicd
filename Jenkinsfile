pipeline {
    agent any
    
    stages {
        stage('Hello World') {
            steps {
                echo 'Hello, World!'
                echo 'Hello, World with IT'
            }
        }
        stage('Welcome to DevOps') {
            steps {
                echo 'Hello, DevOps!'
            }
        }
        stage('Sample Stage') {
            steps {
                echo 'Sample Step 1'
                echo 'Sample Step 2'
            }
        }
    }
}
